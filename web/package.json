{
  "name": "MemberHive",
  "version": "2.0.0",
  "license": "AGPL-3.0",
  "angular-cli": {},
  "repository": {
    "url": "https://github.com/digitaldeacon/memberhive2"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --preserve-symlinks",
    "start:ssl": "ng serve --proxy-config proxy.conf.json --preserve-symlinks --ssl 1 --ssl-key \"/Applications/MAMP/ssl/localhost.key\" --ssl-cert \"/Applications/MAMP/ssl/localhost.cert\"",
    "build:noc": "rm -rf dist/ && ng build --prod",
    "build": "rm -rf dist/ && ng build --prod && gulp compress",
    "extract-i18n": "cd src && ng xi18n",
    "tslint": "tslint \"**/*.ts\" -c src/tslint.json -e \"**/blueprints/*/files/**/*.ts\" -e \"node_modules/**\" -e \"tmp/**\" -e \"dist/**\"",
    "phplint": "cd ../api && ./vendor/bin/phpcs --extensions=php --standard=./vendor/yiisoft/yii2-coding-standards/Yii2/ ./controllers/ ./models/ ./commands/ ./migrations/ ./config/",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "lint": "yarn tslint",
    "install:dep": "yarn link mh-core",
    "inspect:vendor": "rm -rf dist/ && ng build --prod --sm && source-map-explorer dist/vendor*.js",
    "postinstall": "npm rebuild node-sass"
  },
  "engines": {
    "node": ">= 5.4.1",
    "npm": ">= 3.10.7",
    "yarn": ">= 0.24.0"
  },
  "private": true,
  "dependencies": {
    "@agm/core": "^1.0.0-beta.1",
    "@angular/animations": "^5.0.0",
    "@angular/cdk": "angular/cdk-builds",
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/flex-layout": "~2.0.0-beta.10",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/material": "angular/material2-builds",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "@auth0/angular-jwt": "^1.0.0-beta.9",
    "@ngrx/effects": "github:ngrx/effects-builds",
    "@ngrx/router-store": "github:ngrx/router-store-builds",
    "@ngrx/store": "github:ngrx/store-builds",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.67",
    "angular2-moment": "^1.2.0",
    "core-js": "^2.4.1",
    "dragula": "^3.7.2",
    "hammerjs": "^2.0.8",
    "intl": "^1.2.5",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.5.2",
    "lodash": "^4.17.4",
    "material-design-icons": "^3.0.1",
    "ng2-dragula": "^1.3.1",
    "ng2-img-cropper": "^0.9.0",
    "ngx-webstorage": "^1.8.0",
    "reflect-metadata": "^0.1.3",
    "roboto-fontface": "^0.8.0",
    "rxjs": "^5.5.0",
    "tinymce": "^4.5.5",
    "web-animations-js": "^2.2.5",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "1.5.0-rc.2",
    "@angular/compiler-cli": "^5.0.0",
    "@angular/platform-server": "^5.0.0",
    "@ngrx/store-devtools": "github:ngrx/store-devtools-builds",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^8.0.6",
    "@types/tinymce": "^4.5.4",
    "codelyzer": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-brotli": "^1.2.0",
    "gulp-zopfli": "^1.0.0",
    "jasmine-core": "^2.6.2",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.1.1",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ngrx-store-freeze": "^0.2.0",
    "node-sass": "^4.5.3",
    "protractor": "~5.2.0",
    "ts-node": "^3.0.0",
    "tslint": "^5.2.0",
    "typescript": "^2.4.1",
    "yarn-run-all": "^3.1.1"
  }
}
