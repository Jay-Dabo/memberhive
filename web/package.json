{
  "name": "MemberHive",
  "version": "2.0.0",
  "license": "AGPL-3.0",
  "angular-cli": {},
  "repository": {
    "url": "https://github.com/digitaldeacon/memberhive2"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json",
    "build": "rm -rf dist/ && ng build --prod",
    "extract-i18n": "cd src && ng xi18n",
    "tslint": "tslint \"**/*.ts\" -c src/tslint.json -e \"**/blueprints/*/files/**/*.ts\" -e \"node_modules/**\" -e \"tmp/**\" -e \"dist/**\"",
    "phplint": "cd ../api && ./vendor/bin/phpcs --extensions=php --standard=./vendor/yiisoft/yii2-coding-standards/Yii2/ ./controllers/ ./models/ ./commands/ ./migrations/ ./config/",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "lint": "yarn tslint && yarn phplint",
    "install:dep": "yarn link mh-core"
  },
  "engines": {
    "node": ">= 5.4.1",
    "npm": ">= 3.10.7",
    "yarn": ">= 0.21.0"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/flex-layout": "^2.0.0-beta.8",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "https://github.com/angular/material2-builds.git",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/store": "^2.2.1",
    "angular2-moment": "^1.2.0",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "ng2-dragula": "^1.3.1",
    "ng2-img-cropper": "*",
    "ng2-webstorage": "^1.5.0",
    "ngrx-store-freeze": "^0.1.6",
    "ngrx-store-logger": "^0.1.7",
    "reflect-metadata": "^0.1.3",
    "reselect": "^3.0.0",
    "roboto-fontface": "^0.7.0",
    "rxjs": "^5.2.0",
    "tinymce": "^4.5.5",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@ngrx/store-devtools": "^3.2.2",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "2.5.38",
    "@types/node": "^7.0.12",
    "codelyzer": "^2.1.1",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.0.0",
    "typescript": "~2.2.1",
    "yarn-run-all": "^3.1.1"
  }
}
