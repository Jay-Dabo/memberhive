{
  "name": "MemberHive",
  "version": "2.0.0",
  "license": "AGPL-3.0",
  "repository": {
    "url": "https://github.com/digitaldeacon/memberhive2"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --preserve-symlinks",
    "start:ssl": "ng serve --proxy-config proxy.conf.json --preserve-symlinks --ssl 1 --ssl-key \"/Applications/MAMP/ssl/localhost.key\" --ssl-cert \"/Applications/MAMP/ssl/localhost.cert\"",
    "build:noc": "rm -rf dist/ && ng build",
    "build": "rm -rf dist/ && ng build && gulp compress",
    "extract-i18n": "cd src && ng xi18n",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "install:dep": "yarn link mh-core",
    "inspect:vendor": "rm -rf dist/ && ng build --prod --sm && source-map-explorer dist/vendor*.js"
  },
  "private": true,
  "engines": {
    "node": ">= 5.4.1",
    "npm": ">= 3.10.7",
    "yarn": ">= 1.3.0"
  },
  "dependencies": {
    "@agm/core": "^1.0.0-beta.1",
