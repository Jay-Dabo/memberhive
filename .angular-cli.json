{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "memberhivex",
    "npmScope": "memberhivex",
    "latestMigration": "20180227-cleanup-scripts"
  },
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/web/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/web/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics",
      "postGenerate": "npm run format",
      "newProject": [
        "app",
        "lib"
      ]
    },
    "styleExt": "scss",
    "component": {}
  },
  "warnings": {
    "typescriptMismatch": false
  },
  "apps": [
    {
      "name": "web",
      "root": "apps/web/src",
      "outDir": "dist/apps/web",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "mh",
      "styles": [
        "../../../node_modules/roboto-fontface/css/roboto/sass/roboto-fontface-regular.scss",
        "../../../node_modules/material-design-icons/iconfont/material-icons.css",
        "../../../node_modules/dragula/dist/dragula.css",
        "styles.scss",
        "theme.scss"
      ],
      "scripts": [
        "../../../node_modules/tinymce/tinymce.js",
        "../../../node_modules/tinymce/themes/modern/theme.js",
        "../../../node_modules/tinymce/plugins/link/plugin.js",
        "../../../node_modules/tinymce/plugins/paste/plugin.js",
        "../../../node_modules/tinymce/plugins/autoresize/plugin.js",
        "../../../node_modules/tinymce/plugins/lists/plugin.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts",
        "hmr": "environments/environment.hmr.ts"
      }
    },
    {
      "name": "core",
      "root": "libs/core/src",
      "test": "../../../test.js",
      "appRoot": ""
    }
  ]
}