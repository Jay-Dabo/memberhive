{
  "name": "mh-core",
  "version": "2.0.0",
  "license": "AGPL-3.0",
  "angular-cli": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitaldeacon/memberhive2.git"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./aot/* && rm -rf ./dist/* && yarn clean:postbuild",
    "clean:postbuild": "find ./src/ -type f -iname *.ngsummary.json -delete && find ./src/ -type f -iname *.ngfactory.ts -delete",
    "build": "yarn run clean && ngc -p tsconfig.json && yarn run clean:postbuild && yarn lint",
    "build:raw": "yarn run clean && ngc -p tsconfig.json && yarn run clean:postbuild",
    "lint": "tslint \"**/*.ts\" -c tslint.json -e \"**/blueprints/*/files/**/*.ts\" -e \"node_modules/**\" -e \"tmp/**\" -e \"dist/**\"",
    "install:dep": "yarn link"
  },
  "engines": {
    "node": ">= 5.4.1",
    "npm": ">= 3.10.7",
    "yarn": ">= 0.24.0"
  },
  "peerDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/router": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/effects": "github:ngrx/effects-builds",
    "@ngrx/router-store": "github:ngrx/router-store-builds",
    "@ngrx/store": "github:ngrx/store-builds",
    "@types/lodash": "^4.14.67",
    "@types/lodash.isequal": "^4.5.1",
    "@types/node": "^7.0.12",
    "codelyzer": "^3.1.1",
    "lodash": "^4.17.4",
    "ngx-webstorage": "^1.8.0",
    "rxjs": "^5.4.2",
    "tslint": "^5.2.0",
    "typescript": "^2.4.1",
    "zone.js": "^0.8.10"
  }
}
