{
  "name": "mh-core",
  "version": "2.0.0",
  "license": "AGPL-3.0",
  "angular-cli": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digitaldeacon/memberhive2.git"
  },
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf ./aot/* && rm -rf ./dist/* && npm run clean:postbuild",
    "clean:postbuild": "find ./src/ -type f -iname *.ngsummary.json -delete && find ./src/ -type f -iname *.ngfactory.ts -delete",
    "build": "npm run clean && ngc -p tsconfig.json && npm run clean:postbuild",
    "publishPackage": "npm run build && npm publish"
  },
  "engines": {
    "node": ">= 5.4.1",
    "npm": ">= 3.10.7",
    "yarn": ">= 0.21.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "2.0.0",
    "@ngrx/store": "2.2.1",
    "@types/node": "^7.0.5",
    "ng2-webstorage": "^1.5.0",
    "rxjs": "^5.1.0",
    "tslint": "~4.4.2",
    "typescript": "~2.2.1",
    "zone.js": "^0.8.4"
  }
}
